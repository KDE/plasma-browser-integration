#: downloadjob.cpp:135
#, kde-format
msgid ""
"This type of file can harm your computer. If you want to keep it, accept "
"this download from the browser window."
msgstr ""
"סוג כזה של קובץ יכול להזיק למחשב שלך. כדי לשמור אותו, יש לאשר את ההורדה הזאת "
"מחלון הדפדפן."

#: downloadjob.cpp:232
#, kde-format
msgid "Access denied."
msgstr "הגישה נדחתה."

#: downloadjob.cpp:233
#, kde-format
msgid "Insufficient free space."
msgstr "אין מספיק מקום פנוי."

#: downloadjob.cpp:234
#, kde-format
msgid "The file name you have chosen is too long."
msgstr "שם הקובץ שבחרת ארוך מדי."

#: downloadjob.cpp:235
#, kde-format
msgid "The file is too large to be downloaded."
msgstr "הקובץ גדול מכדי שאפשר יהיה להוריד אותו."

#: downloadjob.cpp:237
#, kde-format
msgid "The file possibly contains malicious contents."
msgstr "נראה שתוכן הקובץ הוא זדוני."

#: downloadjob.cpp:238
#, kde-format
msgid "A temporary error has occurred. Please try again later."
msgstr "אירעה שגיאה זמנית. נא לנסות שוב מאוחר יותר."

#: downloadjob.cpp:240
#, kde-format
msgid "A network error has occurred."
msgstr "אירעה שגיאת רשת."

#: downloadjob.cpp:241
#, kde-format
msgid "The network operation timed out."
msgstr "הזמן שהוקצב לפעילות הרשת תם."

#: downloadjob.cpp:242
#, kde-format
msgid "The network connection has been lost."
msgstr "החיבור לרשת אבד."

#: downloadjob.cpp:243
#, kde-format
msgid "The server is no longer reachable."
msgstr "השרת אינו נגיש עוד."

#: downloadjob.cpp:245
#, kde-format
msgid "A server error has occurred."
msgstr "אירעה שגיאת שרת."

#: downloadjob.cpp:249
#, kde-format
msgid "The server does not have the requested data."
msgstr "בשרת אין את הנתונים המבוקשים."

#: downloadjob.cpp:251
#, kde-format
msgid "The browser application closed unexpectedly."
msgstr "יישום דפדפן נסגר במפתיע."

#: downloadjob.cpp:257
#, kde-format
msgid "An unknown error occurred while downloading."
msgstr "אירעה שגיאה בלתי ידועה בזמן ההורדה."

#: downloadjob.cpp:295
#, kde-format
msgctxt "Job heading, like 'Copying'"
msgid "Downloading"
msgstr "הורדה"

#: downloadjob.cpp:296
#, kde-format
msgctxt "The URL being downloaded"
msgid "Source"
msgstr "מקור"

#: downloadjob.cpp:297
#, kde-format
msgctxt "The location being downloaded to"
msgid "Destination"
msgstr "יעד"

#: historyrunnerplugin.cpp:108
#, kde-format
msgctxt "Dummy search result"
msgid "Additional permissions are required"
msgstr "נדרשות הרשאות נוספות"

#: tabsrunnerplugin.cpp:38
#, kde-format
msgid "Mute Tab"
msgstr "השתקת לשונית"

#: tabsrunnerplugin.cpp:43
#, kde-format
msgid "Unmute Tab"
msgstr "ביטול השתקת לשונית"
