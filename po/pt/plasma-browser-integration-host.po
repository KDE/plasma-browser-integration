#: downloadjob.cpp:138
#, kde-format
msgid ""
"This type of file can harm your computer. If you want to keep it, accept "
"this download from the browser window."
msgstr ""
"Este tipo de ficheiro pode danificar o seu computador. Se pretender mantê-"
"lo, aceite esta transferência a partir da janela do navegador."

#: downloadjob.cpp:235
#, kde-format
msgid "Access denied."
msgstr "Acesso negado."

#: downloadjob.cpp:236
#, kde-format
msgid "Insufficient free space."
msgstr "Espaço livre insuficiente."

#: downloadjob.cpp:237
#, kde-format
msgid "The file name you have chosen is too long."
msgstr "O nome de ficheiro que escolheu é demasiado longo."

#: downloadjob.cpp:238
#, kde-format
msgid "The file is too large to be downloaded."
msgstr "O ficheiro é demasiado grande para ser transferido."

#: downloadjob.cpp:240
#, kde-format
msgid "The file possibly contains malicious contents."
msgstr "O ficheiro pode conter conteúdo malicioso."

#: downloadjob.cpp:241
#, kde-format
msgid "A temporary error has occurred. Please try again later."
msgstr "Ocorreu um erro temporário. Por favor, tente novamente mais tarde."

#: downloadjob.cpp:243
#, kde-format
msgid "A network error has occurred."
msgstr "Ocorreu um erro de rede."

#: downloadjob.cpp:244
#, kde-format
msgid "The network operation timed out."
msgstr "A operação de rede expirou."

#: downloadjob.cpp:245
#, kde-format
msgid "The network connection has been lost."
msgstr "A ligação de rede foi perdida."

#: downloadjob.cpp:246
#, kde-format
msgid "The server is no longer reachable."
msgstr "O servidor já não está acessível."

#: downloadjob.cpp:248
#, kde-format
msgid "A server error has occurred."
msgstr "Ocorreu um erro no servidor."

#: downloadjob.cpp:252
#, kde-format
msgid "The server does not have the requested data."
msgstr "O servidor não tem os dados solicitados."

#: downloadjob.cpp:254
#, kde-format
msgid "The browser application closed unexpectedly."
msgstr "A aplicação do navegador fechou inesperadamente."

#: downloadjob.cpp:260
#, kde-format
msgid "An unknown error occurred while downloading."
msgstr "Ocorreu um erro desconhecido durante a transferência."

#: downloadjob.cpp:298
#, kde-format
msgctxt "Job heading, like 'Copying'"
msgid "Downloading"
msgstr "A transferir"

#: downloadjob.cpp:299
#, kde-format
msgctxt "The URL being downloaded"
msgid "Source"
msgstr "Origem"

#: downloadjob.cpp:300
#, kde-format
msgctxt "The location being downloaded to"
msgid "Destination"
msgstr "Destino"

