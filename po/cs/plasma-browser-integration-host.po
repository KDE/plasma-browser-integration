#: downloadjob.cpp:135
#, kde-format
msgid ""
"This type of file can harm your computer. If you want to keep it, accept "
"this download from the browser window."
msgstr ""

#: downloadjob.cpp:232
#, kde-format
msgid "Access denied."
msgstr "Přístup odmítnut."

#: downloadjob.cpp:233
#, kde-format
msgid "Insufficient free space."
msgstr "Nedostatek volného místa."

#: downloadjob.cpp:234
#, kde-format
msgid "The file name you have chosen is too long."
msgstr "Vybraný název souboru je příliš dlouhý."

#: downloadjob.cpp:235
#, kde-format
msgid "The file is too large to be downloaded."
msgstr "Soubor je příliš velký pro stažení."

#: downloadjob.cpp:237
#, kde-format
msgid "The file possibly contains malicious contents."
msgstr "Soubor možná obsahuje škodlivý obsah."

#: downloadjob.cpp:238
#, kde-format
msgid "A temporary error has occurred. Please try again later."
msgstr "Došlo  k dočasné chybě. Prosím zkuste to později."

#: downloadjob.cpp:240
#, kde-format
msgid "A network error has occurred."
msgstr "Došlo k chybě sítě."

#: downloadjob.cpp:241
#, kde-format
msgid "The network operation timed out."
msgstr "Čas na síťovou operaci vypršel."

#: downloadjob.cpp:242
#, kde-format
msgid "The network connection has been lost."
msgstr "Síťové spojení bylo ztraceno."

#: downloadjob.cpp:243
#, kde-format
msgid "The server is no longer reachable."
msgstr "Server již není dosažitelný."

#: downloadjob.cpp:245
#, kde-format
msgid "A server error has occurred."
msgstr "Došlo k chybě serveru."

#: downloadjob.cpp:249
#, kde-format
msgid "The server does not have the requested data."
msgstr "Server nemá požadované data."

#: downloadjob.cpp:251
#, kde-format
msgid "The browser application closed unexpectedly."
msgstr "Prohlížeč nečekaně skončil."

#: downloadjob.cpp:257
#, kde-format
msgid "An unknown error occurred while downloading."
msgstr "Při stahování došlo k neznámé chybě."

#: downloadjob.cpp:295
#, kde-format
msgctxt "Job heading, like 'Copying'"
msgid "Downloading"
msgstr "Stahuji"

#: downloadjob.cpp:296
#, kde-format
msgctxt "The URL being downloaded"
msgid "Source"
msgstr "Zdroj"

#: downloadjob.cpp:297
#, kde-format
msgctxt "The location being downloaded to"
msgid "Destination"
msgstr "Cíl"

#: historyrunnerplugin.cpp:108
#, kde-format
msgctxt "Dummy search result"
msgid "Additional permissions are required"
msgstr "Je potřeba další povolení"

#: tabsrunnerplugin.cpp:38
#, kde-format
msgid "Mute Tab"
msgstr "Ztlumit kartu"

#: tabsrunnerplugin.cpp:43
#, kde-format
msgid "Unmute Tab"
msgstr "Povolit zvuk na kartě"
