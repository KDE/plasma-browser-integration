#: downloadjob.cpp:135
#, kde-format
msgid ""
"This type of file can harm your computer. If you want to keep it, accept "
"this download from the browser window."
msgstr ""
"Este tipo de ficheiro pode danar o seu computador. Se quere quedar con el, "
"acepte esta descarga desde a xanela do navegador."

#: downloadjob.cpp:232
#, kde-format
msgid "Access denied."
msgstr "Acceso denegado."

#: downloadjob.cpp:233
#, kde-format
msgid "Insufficient free space."
msgstr "Espazo libre insuficiente."

#: downloadjob.cpp:234
#, kde-format
msgid "The file name you have chosen is too long."
msgstr "O nome de ficheiro que escolleu é demasiado longo."

#: downloadjob.cpp:235
#, kde-format
msgid "The file is too large to be downloaded."
msgstr "O ficheiro é grande de máis para descargalo."

#: downloadjob.cpp:237
#, kde-format
msgid "The file possibly contains malicious contents."
msgstr "O ficheiro podería ter contido malicioso."

#: downloadjob.cpp:238
#, kde-format
msgid "A temporary error has occurred. Please try again later."
msgstr "Produciuse un erro temporal. Inténteo de novo máis tarde."

#: downloadjob.cpp:240
#, kde-format
msgid "A network error has occurred."
msgstr "Produciuse un erro de rede."

#: downloadjob.cpp:241
#, kde-format
msgid "The network operation timed out."
msgstr "A operación de rede caducou."

#: downloadjob.cpp:242
#, kde-format
msgid "The network connection has been lost."
msgstr "Perdeuse a conexión de rede."

#: downloadjob.cpp:243
#, kde-format
msgid "The server is no longer reachable."
msgstr "Non é posíbel acadar o servidor."

#: downloadjob.cpp:245
#, kde-format
msgid "A server error has occurred."
msgstr "Produciuse un erro de servidor."

#: downloadjob.cpp:249
#, kde-format
msgid "The server does not have the requested data."
msgstr "O servidor non dispón dos datos solicitados."

#: downloadjob.cpp:251
#, kde-format
msgid "The browser application closed unexpectedly."
msgstr "A aplicación de navegador pechouse inesperadamente."

#: downloadjob.cpp:257
#, kde-format
msgid "An unknown error occurred while downloading."
msgstr "Produciuse un erro descoñecido ao descargar."

#: downloadjob.cpp:295
#, kde-format
msgctxt "Job heading, like 'Copying'"
msgid "Downloading"
msgstr "Descargando"

#: downloadjob.cpp:296
#, kde-format
msgctxt "The URL being downloaded"
msgid "Source"
msgstr "Orixe"

#: downloadjob.cpp:297
#, kde-format
msgctxt "The location being downloaded to"
msgid "Destination"
msgstr "Destino"

#: historyrunnerplugin.cpp:108
#, kde-format
msgctxt "Dummy search result"
msgid "Additional permissions are required"
msgstr "Precísanse permisos adicionais"

#: tabsrunnerplugin.cpp:38
#, kde-format
msgid "Mute Tab"
msgstr "Silenciar o separador"

#: tabsrunnerplugin.cpp:43
#, kde-format
msgid "Unmute Tab"
msgstr "Activar o son do separador"
